{
  "name": "blobmaker",
  "description": "Generate blobs with ease",
  "version": "0.2.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "BlobMaker",
    "manifest": "src/manifest.json",
    "main": "blobmaker.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Federico Vitale <hi@fedevitale.dev>",
  "dependencies": {
    "blobmaker": "^0.0.1"
  }
}
